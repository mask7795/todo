{
  "name": "todo-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 127.0.0.1 --port 4200 --proxy-config proxy.conf.json --verbose --hmr=false",
    "start:backend": "../scripts/start-backend.sh",
    "start:frontend": "ng serve --host 127.0.0.1 --port 4200 --proxy-config proxy.conf.json --verbose --hmr=false",
    "start:frontend:4300": "ng serve --host 127.0.0.1 --port 4300 --proxy-config proxy.conf.json --verbose --hmr=false",
    "start:frontend:sh": "../scripts/start-frontend.sh",
    "setup:frontend:sh": "../scripts/setup-frontend.sh",
    "start:dev": "concurrently -k -n backend,frontend -c green.bold,blue.bold \"npm run start:backend\" \"npm run start:frontend\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.9.2",
  "engines": {
    "node": ">=20 <21",
    "npm": ">=10 <11"
  },
  "dependencies": {
    "@angular/common": "21.0.3",
    "@angular/compiler": "21.0.3",
    "@angular/core": "21.0.3",
    "@angular/forms": "21.0.3",
    "@angular/platform-browser": "21.0.3",
    "@angular/router": "21.0.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "21.0.2",
    "@angular/cli": "21.0.2",
    "@angular/compiler-cli": "21.0.3",
    "@playwright/test": "^1.57.0",
    "concurrently": "^9.0.0",
    "jsdom": "^27.1.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
